<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
  <% include ./partials/_header %>
</head>
<body>
    <!-- Add your EJS code here -->
    <h1>Your URLS</h1>
      <ul>
        <% Object.keys(urls).forEach(function(key) { %>
          <li>
            tiny.app/<%=key%>
            -
            <a href="<%=urls[key]%>">
              <%= urls[key] %>
            </a>
          </li>
        <% }); %>
      </ul>
    <a href="urls/new">Click here to shorten your own link.</a>
</body>
</html>
